{
    "title": "CasperID LLM Health",
    "description": "Comprehensive observability dashboard for CasperID's Gemini-powered AI verification system",
    "widgets": [
        {
            "id": 1,
            "definition": {
                "type": "query_value",
                "requests": [
                    {
                        "q": "avg:casperid.gemini.latency{endpoint:verify-kyc}",
                        "aggregator": "avg"
                    }
                ],
                "title": "Avg KYC Latency",
                "title_size": "16",
                "title_align": "left",
                "precision": 0,
                "custom_unit": "ms"
            },
            "layout": {
                "x": 0,
                "y": 0,
                "width": 3,
                "height": 2
            }
        },
        {
            "id": 2,
            "definition": {
                "type": "timeseries",
                "requests": [
                    {
                        "q": "sum:casperid.verification.result{outcome:success}.as_count()",
                        "display_type": "bars",
                        "style": {
                            "palette": "green"
                        }
                    },
                    {
                        "q": "sum:casperid.verification.result{outcome:failed}.as_count()",
                        "display_type": "bars",
                        "style": {
                            "palette": "red"
                        }
                    }
                ],
                "title": "Verification Success vs Failed",
                "show_legend": true
            },
            "layout": {
                "x": 3,
                "y": 0,
                "width": 6,
                "height": 2
            }
        },
        {
            "id": 3,
            "definition": {
                "type": "query_value",
                "requests": [
                    {
                        "q": "sum:casperid.gemini.calls.total{*}",
                        "aggregator": "sum"
                    }
                ],
                "title": "Total AI Calls Today",
                "title_size": "16",
                "precision": 0
            },
            "layout": {
                "x": 9,
                "y": 0,
                "width": 3,
                "height": 2
            }
        },
        {
            "id": 4,
            "definition": {
                "type": "timeseries",
                "requests": [
                    {
                        "q": "avg:casperid.gemini.confidence{endpoint:verify-kyc}",
                        "display_type": "line",
                        "style": {
                            "palette": "blue"
                        }
                    },
                    {
                        "q": "avg:casperid.gemini.confidence{endpoint:verify-liveness}",
                        "display_type": "line",
                        "style": {
                            "palette": "purple"
                        }
                    }
                ],
                "title": "AI Confidence Scores Over Time",
                "show_legend": true,
                "yaxis": {
                    "min": "0",
                    "max": "1"
                }
            },
            "layout": {
                "x": 0,
                "y": 2,
                "width": 6,
                "height": 3
            }
        },
        {
            "id": 5,
            "definition": {
                "type": "toplist",
                "requests": [
                    {
                        "q": "top(sum:casperid.verification.result{outcome:failed} by {reason}, 10, 'sum', 'desc')"
                    }
                ],
                "title": "Top Rejection Reasons"
            },
            "layout": {
                "x": 6,
                "y": 2,
                "width": 6,
                "height": 3
            }
        },
        {
            "id": 6,
            "definition": {
                "type": "query_value",
                "requests": [
                    {
                        "q": "sum:casperid.fraud.attempts{*}",
                        "aggregator": "sum"
                    }
                ],
                "title": "Fraud Attempts Blocked",
                "title_size": "16",
                "precision": 0,
                "custom_unit": "attempts"
            },
            "layout": {
                "x": 0,
                "y": 5,
                "width": 3,
                "height": 2
            }
        },
        {
            "id": 7,
            "definition": {
                "type": "timeseries",
                "requests": [
                    {
                        "q": "sum:casperid.gemini.calls.total{result:error}.as_rate()",
                        "display_type": "line",
                        "style": {
                            "palette": "warm",
                            "line_type": "solid",
                            "line_width": "normal"
                        }
                    }
                ],
                "title": "Error Rate",
                "yaxis": {
                    "min": "0"
                }
            },
            "layout": {
                "x": 3,
                "y": 5,
                "width": 9,
                "height": 2
            }
        }
    ],
    "template_variables": [
        {
            "name": "env",
            "default": "development",
            "prefix": "env"
        },
        {
            "name": "endpoint",
            "default": "*",
            "prefix": "endpoint"
        }
    ],
    "layout_type": "ordered",
    "is_read_only": false,
    "notify_list": [],
    "reflow_type": "fixed"
}